{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-router",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "params": {
    "envid": {
      "required": true,
      "value": "foobarbaz-id"
    }
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "app": {
    "styles": []
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
